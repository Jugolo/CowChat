<!DOCTYPE html>
<html>
 <head>
  @-if exist title-@
    <title>@-echo title-@</title>
  @-end-@
  <script>
   const onload = [];
   function load(){
     startChat(
       document.getElementById("txt"),
       StarGui
     );
   }
   window.onerror = function(msg, url, linenumber) {
    @-if __FILE__ equel "main.style"-@
     sys.getPage("console").line("", "", "JavascriptError", "[color=red]"+msg+". On line "+linenumber+" In file "+url+"[/color]");
    @-else-@
    alert('Error message: '+msg+'\nURL: '+url+'\nLine Number: '+linenumber);
    @-end-@
    return true;
   }
   @-trigger client.javascript.end-@
  </script>
  <link rel="stylesheet" type="text/css" href="@-echo __DIR__-@css/style.css">
  @-if exist error-@
  <script>
   alert("Error: @-echo error-@");
  </script>
  @-end-@
  @-if exist js-@
  @-foreach dir : js-@
  <script src='@-echo dir-@'></script>
  @-end-@
  @-end-@
  @-if __FILE__ equel "main.star"-@
  <script src='@-echo __DIR__-@js/gui.js'></script>
  @-end-@
  @-if exist rawjs-@
  <script>
    @-echo rawjs-@
  </script>
  @-end-@
</head>
<body onload='load();'>
